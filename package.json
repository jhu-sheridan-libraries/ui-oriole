{
  "name": "@folio/oriole",
  "version": "0.1.0",
  "description": "Library Database Manager",
  "main": "src/index.js",
  "repository": "",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "start": "stripes serve",
    "build": "stripes build --output ./output",
    "test": "stripes test nightmare --run demo --show",
    "lint": "eslint src"
  },
  "devDependencies": {
    "@folio/eslint-config-stripes": "^1.1.0",
    "babel-eslint": "^8.0.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "eslint": "^4.8.0"
  },
  "dependencies": {
    "@folio/stripes-components": "^2.0.0",
    "prop-types": "^15.6.0",
    "react-intl": "^2.4.0",
    "react-router-dom": "^4.1.1"
  },
  "peerDependencies": {
    "react": "*"
  },
  "stripes": {
    "type": "app",
    "displayName": "Oriole",
    "route": "/oriole",
    "hasSettings": true,
    "okapiInterfaces": {},
    "permissionSets": [
      {
        "permissionName": "module.oriole.enabled",
        "displayName": "UI: Oriole module is enabled",
        "visible": true
      },
      {
        "permissionName": "settings.oriole.enabled",
        "displayName": "Settings (oriole): display list of settings pages",
        "subPermissions": [
          "settings.enabled"
        ],
        "visible": true
      }
    ]
  }
}
